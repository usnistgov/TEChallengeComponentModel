module powerflow {
     solver_method NR;
     NR_iteration_limit 50;
};
module tape;


clock {
     timezone PST+8PDT;
     starttime '2009-01-01 00:00:00';
     stoptime '2009-01-01 00:00:10';
}

///////////////////////////////
// Player
//////////////////////////////
class player {
      double value;
}

object player {
    name PHASE_A_REAL;
	file sub_power_real.player;
    loop 1;
}

object player {
    name PHASE_A_IMAG;
	file sub_power_imag.player;
    loop 1;
}

///////////////////////////////


///////////////////////////////
// Grid
//////////////////////////////

object node {
	name feeder_head;
	bustype SWING;
	phases ABCN;
    nominal_voltage 132790;
}

object transformer {
     name substation_transformer;
     from feeder_head;
     to substation_meter;
     phases ABCN;
     configuration substation_transformer_config;
}

object transformer_configuration {
    	name substation_transformer_config;
    	connect_type WYE_WYE;
     	install_type PADMOUNT;
     	primary_voltage 132790;
     	secondary_voltage 7216;
     	power_rating 8.4 MVA;
     	impedance 0.00033+0.0022j;
     }

object meter {     
      name substation_meter;     
      phases ABCN;     
      nominal_voltage 7216.88;     
      object recorder {
     	name substation_recorder;
     	file substation_data.csv;
     	interval 60;
     	property measured_voltage_A, measured_current_A, measured_power_A;
	};     
} 

object overhead_line {     
      name feeder_backbone;     
      phases ABCN;     
      from substation_meter;     
      to load_meter;     
      length 10000;     
      configuration line_configuration_1;     
} 

object line_configuration {     
      name line_configuration_1;     
      conductor_A overhead_line_conductor_1;     
      conductor_B overhead_line_conductor_1;     
      conductor_C overhead_line_conductor_1;     
      conductor_N overhead_line_conductor_1;     
      spacing line_spacing_1;     
} 

object line_spacing {     
      name line_spacing_1;     
      distance_AB 57.6 in;    
      distance_BC 57.6 in;    
      distance_AC 57.6 in;    
      distance_AN 51.6 in;    
      distance_BN 51.6 in;    
      distance_CN 51.6 in;    
}  

object overhead_line_conductor {     
      name overhead_line_conductor_1;      
      rating.summer.continuous 256.0;     
      geometric_mean_radius 0.01200 ft;    
      resistance 0.1;     
}  

object meter {     
      name load_meter;     
      phases ABCN;     
      nominal_voltage 7216.88;     
      object recorder {
     	name load_recorder;
     	file load_data.csv;
     	interval 60;
     	property measured_voltage_A, measured_current_A, measured_power_A;
	};
} 


///////////////////////////////
// Loads
//////////////////////////////

object load {
     parent load_meter;
     name load1;
     nominal_voltage 7216.88;
     phases ABC;
	constant_power_A 1.0E6+0.0j;
	constant_power_B 1.0E6+0.0j;
	constant_power_C 1.0E6+0.0j;
}


object load {
     parent load_meter;
     name load2;
     nominal_voltage 7216.88;
     phases ABC;
	constant_power_A 1.0E6+0.0j;
	constant_power_B 1.0E6+0.0j;
	constant_power_C 1.0E6+0.0j;
}

object load {
     parent load_meter;
     name load3;
     nominal_voltage 7216.88;
     phases ABC;
	constant_power_A 1.0E6+0.0j;
	constant_power_B 1.0E6+0.0j;
	constant_power_C 1.0E6+0.0j;
}

object load {
     parent load_meter;
     name load4;
     nominal_voltage 7216.88;
     phases ABC;
	constant_power_A_real PHASE_A_REAL.value;
	constant_power_A_reac PHASE_A_IMAG.value;
	constant_power_B_real PHASE_A_REAL.value;
	constant_power_B_reac PHASE_A_IMAG.value;
	constant_power_C_real PHASE_A_REAL.value;
	constant_power_C_reac PHASE_A_IMAG.value;
}

object multi_recorder {
    property substation_meter:measured_power_A, load_meter:measured_power_A, load_meter:measured_power_B,load_meter:measured_power_C;
    file node_data.csv;
    interval 1;
}

